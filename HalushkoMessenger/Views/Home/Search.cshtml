@model HalushkoMessenger.ViewModels.SearchUserViewModel

@{
    ViewData["Title"] = "Search";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>PeopleList</h1>
<div id="renderBody">
    <h1>Our users</h1>
    <div class="userList">
        Chose user to start chating :
        @foreach (var user in Model.Users)
        {
            <div class="container">
                @Html.ActionLink("Search", "Home", user)
            </div>
        }

    </div>
</div>
